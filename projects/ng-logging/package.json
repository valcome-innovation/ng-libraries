{
  "name": "@valcome/ng-logging",
  "version": "1.5.0",
  "description": "Used for logging",
  "scripts": {
    "preversion": "ng test ng-logging --watch=false",
    "version": "ng build ng-logging --configuration production",
    "postversion": "git add . && git commit -m \"Upgrade npm version\" && git push ",
    "publish:patch": "npm version patch && cd ../../dist/ng-logging && npm publish",
    "publish:minor": "npm version minor && cd ../../dist/ng-logging && npm publish",
    "publish:major": "npm version major && cd ../../dist/ng-logging && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valcome-innovation/ng-libraries/tree/master/projects/ng-logging"
  },
  "author": "Valcome Analytics",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/valcome-innovation/ng-libraries/issues"
  },
  "homepage": "https://github.com/valcome-innovation/ng-libraries/tree/master/projects/ng-logging#nglogging",
  "peerDependencies": {
    "@angular/common": "~17.3.0",
    "@angular/core": "~17.3.0",
    "@sentry/angular": "^8.50.0"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  }
}
